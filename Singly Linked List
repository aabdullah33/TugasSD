public class SLL 
{  
 Node head; 
 static class Node {  
 int data;  
         Node next;  
 Node(int d)  { data = d;  next=null; }  
     }  
 public void display()  
     {  
         Node n = head;  
 while (n != null)  
         {  
 System.out.print(n.data+" \n");  
             n = n.next;  
         }  
     }    
 public static void main(String[] args)  
     {   
 SLL list = new SLL();  
  
 list.head       = new Node(11);  
         Node second      = new Node(12);  
         Node third       = new Node(13);  
  
 list.head.next = second;
 second.next = third;
 list.display();  
     }  
}
